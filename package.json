{
  "name": "strimpack",
  "description": "node server for strimpack auth, ssr",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "babel-preset-react-app": "^3.1.1",
    "connect-pg-simple": "^4.2.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "node-fetch": "^2.1.2",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.2",
    "react-addons-update": "^15.6.2",
    "react-dom": "^16.3.2",
    "sequelize": "^4.37.6",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "dotenv": "^5.0.1",
    "ignore-styles": "^5.0.1"
  },
  "scripts": {
    "sync": "node --require 'dotenv/config' --require 'babel-register' ./server/sync.js",
    "start": "nodemon --exec 'node --require dotenv/config --require babel-register --inspect=9229 server/run.js'",
    "build": "babel server -d dist",
    "serve": "node --require 'dotenv/config' dist/run.js"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
