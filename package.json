{
  "name": "strimpack",
  "description": "node server for strimpack auth, ssr",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "connect-pg-simple": "^4.2.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "node-fetch": "^2.1.2",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.2",
    "react-addons-update": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.1",
    "sequelize": "^4.37.6"
